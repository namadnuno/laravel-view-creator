{
	"name": "laravel-view-creator",
	"publisher": "laravel-view-creator",
	"displayName": "Laravel View Creator",
	"description": "Simple and easy way to create an view in laravel projects",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/namadnuno/laravel-view-creator.git"
	},
	"activationEvents": [
		"onCommand:extension.laravel-view-creator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.laravel-view-creator",
				"title": "Laravel - Create View"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/q": "^1.5.2",
		"mkdirp": "^0.5.1",
		"q": "^1.5.1"
	},
	"icon": "icon.png"
}